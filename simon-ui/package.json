{
  "version": "0.3.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "write": "yarn prettier --write ./src",
    "write-example": "yarn prettier --write ./example/src && cd example && yarn write && cd ..",
    "check": "yarn prettier --check ./src"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn write"
    }
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "name": "simon-ui",
  "author": "Simon Boisset",
  "module": "dist/simon-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonboisset/simon-ui",
    "directory": "simon-ui"
  },
  "bugs": {
    "url": "https://github.com/simonboisset/simon-ui/issues"
  },
  "homepage": "https://github.com/simonboisset/simon-ui/tree/master/simon-ui",
  "size-limit": [
    {
      "path": "dist/simon-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/simon-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@size-limit/preset-small-lib": "^4.7.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/yup": "^0.29.9",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@date-io/dayjs": "1.x",
    "@emotion/css": "^11.0.0",
    "@hookform/resolvers": "^1.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/pickers": "^3.2.10",
    "dayjs": "^1.9.5",
    "react-hook-form": "^6.9.6",
    "react-router-url": "^0.2.9",
    "yup": "^0.29.3"
  }
}
